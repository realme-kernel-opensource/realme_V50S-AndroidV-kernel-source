&extcon_usb {
	extcon_usb_2: extcon_usb_2 {
		compatible = "mediatek,extcon-usb";
		charger = <&sy6974b>;
		status = "okay";
		/*vbus-supply = <&mt6375_otg_vbus>;
		vbus-voltage = <5000000>;
		vbus-current = <1800000>;*/
		tcpc = "type_c_port0";
		mediatek,bypss-typec-sink = <1>;
		mediatek,u2;
		port {
			usb_role: endpoint@0 {
				remote-endpoint = <&mtu3_drd_switch>;
			};
		};
	};
};
